{
    "runtime-lib": [
        "/resource/cleopatra.js",
        "/resource/conf/defaults.js"
    ],
    "runtime-css": ["/resource/css/cleopatra.css"],
    "fileupload": {
        "maxbodysize": -1,
        "maxfilesize": -1,
        "tempdirpath": "",
        "thresholdsize": 10240
    },
    "html-lang": "ko",
    "cache-control": false,
    "type-handler":{
        "dateformat": "yyyyMMddHHmmss"
    },
    "deploy-path": ["/ui"]
}